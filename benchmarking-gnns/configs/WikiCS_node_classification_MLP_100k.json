{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "MLP",
    "dataset": "WikiCS",
    
    "out_dir": "out/WikiCS_node_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "init_lr": 1e-2,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-5,
        "weight_decay": 5e-4,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 125,
        "out_dim": 125,
        "readout": "mean",
        "gated": false,
        "in_feat_dropout": 0.0,
        "dropout": 0.35
    }
}