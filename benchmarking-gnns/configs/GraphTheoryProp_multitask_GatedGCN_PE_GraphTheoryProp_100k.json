{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GatedGCN",
    "dataset": "GraphTheoryProp",
    
    "out_dir": "out/GraphTheoryProp_multitask_PE/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 25,
        "init_lr": 1e-3,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 15,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 8,
        "hidden_dim": 40,
        "out_dim": 40,
        "residual": true,
        "readout": "sum",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "pos_enc": true,
        "pos_enc_dim": 12,
        "use_gru": true,
        "architecture": "standard"
    }
}